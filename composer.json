{
    "name": "jor3l/bootstrap-form",
    "description": "Laravel 5 form wrappers for Bootstrap 3.",
    "keywords": ["laravel", "form", "bootstrap"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dwight Watson",
            "email": "dwightwatson@me.com"
        }, { "name": "Jose V", "email": "jose@jose.com.co" }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/config": "~5.0",
        "illuminate/session": "~5.0",
        "illuminate/support": "~5.0",
        "laravelcollective/html": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Jor3l\\BootstrapForm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jor3l\\BootstrapForm\\BootstrapFormServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
